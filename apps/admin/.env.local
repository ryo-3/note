# Petaboo 管理画面 環境変数

# 管理者ログインパスワード
NEXT_PUBLIC_ADMIN_PASSWORD=admin123

# APIサーバーURL
NEXT_PUBLIC_API_URL=http://localhost:7594

# 開発環境での自動ログイン（本番では無効化すること）
NEXT_PUBLIC_AUTO_LOGIN=true

# ローカルアクセス制限（本番環境では必ずtrueにすること）
LOCAL_ACCESS_ONLY=true

# Next.jsの開発インジケーターを無効化
__NEXT_DISABLE_OVERLAY=1
__NEXT_DISABLE_DEV_WARNINGS=1